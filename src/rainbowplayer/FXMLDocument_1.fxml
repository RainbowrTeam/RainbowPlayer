<AnchorPane id="AnchorPane" prefHeight="720" prefWidth="1280.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="rainbowplayer.FXMLDocumentController">
 <children>
  <GridPane prefHeight="720.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
   <children>
        <FlowPane prefHeight="200.0" GridPane.rowIndex="1" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Button  onAction="#handleButtonAction" text="1">
             <FlowPane.margin>
              <Insets right="10.0" />
              <Insets left="10.0" />
             </FlowPane.margin>
            </Button>
            <Button  onAction="#handleButtonAction" text="2">
             <FlowPane.margin>
              <Insets right="10.0" />
              <Insets left="10.0" />
             </FlowPane.margin>
            </Button>
            <Button  onAction="#handleButtonAction" text="3">
             <FlowPane.margin>
              <Insets right="10.0" />
              <Insets left="10.0" />
             </FlowPane.margin>
            </Button>
            <Button  onAction="#handleButtonAction" text="4">
             <FlowPane.margin>
              <Insets right="10.0" />
              <Insets left="10.0" />
             </FlowPane.margin>
            </Button>
         </children>
        </FlowPane>
        <Pane prefHeight="520" AnchorPane.bottomAnchor="0.0" GridPane.rowIndex="2">
         <children>
          <TabPane prefHeight="520" prefWidth="1280" tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0">
           <tabs>
                <Tab text="Playback">
                 <content>
                    <FlowPane hgap="10.0" vgap="10.0">
                     <children>
                        <FlowPane prefHeight="28" prefWidth="800">
                      <children>
                        <Label fx:id="titleLabel" layoutX="1.0" layoutY="14.0" maxWidth="800" text="Title">
                         <FlowPane.margin>
                          <Insets top="10" />
                         </FlowPane.margin>
                         <font>
                          <Font name="System Bold" size="24.0" />
                         </font>
                        </Label> 
                        <Label fx:id="authorLabel" layoutX="1.0" layoutY="14.0" maxWidth="800" text="Author">
                         <FlowPane.margin>
                          <Insets top="10" />
                         </FlowPane.margin>
                         <font>
                          <Font name="System Bold" size="18.0" />
                         </font>
                        </Label> 
                      </children>
                        </FlowPane>
                        <Button  onAction="#handleButtonAction" text="settings">
                         <FlowPane.margin>
                          <Insets left="2000" />
                          <Insets top="10.0" />
                         </FlowPane.margin>
                        </Button>                
                   </children>
                    </FlowPane>
                 </content>
                </Tab>
                <Tab text="Playlist">
                 <content>
                  <FlowPane>
                   <children>
                   </children>
                  </FlowPane>
                 </content>
                </Tab>
           </tabs>
          </TabPane>              
         </children>
        </Pane>
   </children>
  </GridPane>
 </children>
</AnchorPane>
 