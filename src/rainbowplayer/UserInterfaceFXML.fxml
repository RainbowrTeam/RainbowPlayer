                 </padding>
                  <font>
                  <Font name="System Bold" size="12.0" />
                 </font>
                 <tooltip>
                  <Tooltip text="total time" />
                 </tooltip>
              </Label>
             <Slider id="Child-ProgressSlider" orientation="HORIZONTAL" prefWidth="1200.0" GridPane.rowIndex="2" GridPane.columnIndex="1" GridPane.columnSpan="3"/>
