              <Font name="fontawesome-webfont" size="24.0" />
             </font>
             <tooltip>
              <Tooltip id="Att-PrevButtonTooltip" text="previous song" />
             </tooltip>
            </Button>
            <ToggleButton id="Child-PlayButton" prefHeight="70.0" prefWidth="70.0" onAction="#handlePauseButtonAction" GridPane.columnIndex="2"> 
